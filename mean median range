a<-c(5,6,7,8,9,10,11,12)
b<-c(8,7,6,5,90,776,32,56)
df<-data.frame(a,b)
mean1<-mean(df $a)
mean2<-mean(df $b)
median1<-median(df $a)
median2<-median(df $b)
range1<-range(df $a)
range2<-range(df $b)
ifelse(mean1>mean2,print("class A has higher."),print("class b has higher"))
ifelse(median1>median2,print("class A has higher."),print("class b has higher"))
ifelse(range1>range2,print("class A has higher."),print("class b has higher"))
write.csv(df,"datafr.csv")
